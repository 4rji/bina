#!/usr/bin/env bash
echo ""
# Descomprime el archivo ZIP
unzip -d . /opt/4rji/bin/GoldenEye-master.zip

# Cambia al directorio descomprimido
cd /tmp/GoldenEye-master

# Asegura que el script Python tiene permisos de ejecuci√≥n
chmod +x goldeneye.py

echo ""
echo ""

echo -e "\e[1;32mEjecuta el script Python\e[0m"
echo -e "\e[1;34mcd GoldenEye-master\e[0m"
echo -e "\e[1;34m./goldeneye.py http://IP\e[0m"

echo ""
echo -e "\e[1;33mEjemplo de uso:\e[0m"
echo -e "\e[1;36m./goldeneye.py https://objetivo.com -w 50 -s 1000 -m random -n\e[0m"
echo ""